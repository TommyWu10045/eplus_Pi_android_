<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="send_way_edit_1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">@dimen/stand_font_m</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/bg_edit_5</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/gray1</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>




    <style name="add_address_1">
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:textSize">@dimen/stand_font_s</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/bg_edit_5</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/gray1</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>






    <style name="add_address_2">
        <item name="android:paddingLeft">15dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:textSize">@dimen/stand_font_m</item>
        <item name="android:layout_weight">2</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:layout_gravity">right|center_vertical</item>
        <item name="android:textColor">@color/base_font_1</item>
    </style>




    <style name="add_address_3">
        <item name="android:paddingLeft">15dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:textSize">@dimen/stand_font_m</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:layout_gravity">right|center_vertical</item>
        <item name="android:textColor">@color/base_font_1</item>
    </style>






    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="myAppThemeDialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="adapter_home_text1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/stand_font_s</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="main_map_1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>


    <style name="pi_buy_key_num">

        <item name="android:layout_margin">1dp</item>
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">24sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/btn_num_top_press</item>
    </style>


    <style name="pi_buy_key_btn">
        <item name="android:layout_margin">5dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/btn_num_top_press</item>
    </style>


</resources>
